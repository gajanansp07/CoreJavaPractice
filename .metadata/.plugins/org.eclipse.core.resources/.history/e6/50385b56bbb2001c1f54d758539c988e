package loopingInJava;

public class ForLoopExpl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// multiple table of 2
		//Range Based loop
//		Multiplication table of 2
		for(int i=1;i<=10;i++) {
			System.out.println(i*12+"    "+i*13+"    "+i*14+"    "+i*15+"    "+i*16+"    "+i*17+"    "+i*18+"    "+i*19);
		}

	}

}
