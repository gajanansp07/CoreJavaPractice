package loopingInJava;

public class ForLoopExpl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// multiple table of 2
		//Range Based loop
//		Multiplication table of 2
		for(int i=1;i<=10;i++) {
			System.out.println(i*2+"  "+i*3+"  "+i*4+"  "+i*5+"  "+i*6+"  "+i*7+"  "+i*8+"  "+i*9);
		}

	}

}
